{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "extends": ["google", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-unused-vars": "off",
    "require-jsdoc": "off",
    "spaced-comment": "off"
  }
}
